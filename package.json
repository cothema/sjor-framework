{
  "name": "@cothema/sjor-framework",
  "version": "0.3.2",
  "description": "Simple framework built with Next.js and Tailwind CSS, designed for fast creation of landing pages.",
  "main": "dist/ssr.js",
  "module": "dist/ssr.mjs",
  "types": "dist/ssr.d.ts",
  "exports": {
    ".": {
      "import": "./dist/ssr.mjs",
      "require": "./dist/ssr.js",
      "types": "./dist/ssr.d.ts"
    },
    "./client": {
      "import": "./dist/client.mjs",
      "require": "./dist/client.js",
      "types": "./dist/client.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/cothema/sjor-framework.git"
  },
  "peerDependencies": {
    "@cothema/drakkar-ui": "^0.5.1",
    "@fortawesome/fontawesome-svg-core": ">=6.7.2",
    "@fortawesome/free-brands-svg-icons": ">=6.7.2",
    "@fortawesome/free-solid-svg-icons": ">=6.7.2",
    "@fortawesome/react-fontawesome": ">=0.2.2",
    "next": ">=15.1.4",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "swiper": "^11.2.5",
    "tailwindcss": ">=3.0.0"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^3.2.5",
    "@storybook/addon-essentials": "^8.6.4",
    "@storybook/addon-onboarding": "^8.6.4",
    "@storybook/blocks": "^8.6.4",
    "@storybook/experimental-addon-test": "^8.6.4",
    "@storybook/nextjs": "^8.6.4",
    "@storybook/test": "^8.6.4",
    "@types/storybook__react": "^4.0.2",
    "autoprefixer": "^10.4.20",
    "playwright": "^1.50.1",
    "postcss": "^8.5.3",
    "standard-version": "^9.5.0",
    "storybook": "^8.6.4",
    "tailwindcss": "^3.4.17",
    "tsup": "^8.4.0",
    "typescript": "^5.8.2"
  },
  "scripts": {
    "build": "tsup",
    "build-storybook": "storybook build",
    "dev": "tsup --watch",
    "prepare": "npm run build",
    "release": "node generate-index.mjs && git diff --quiet || (echo '‚ùå Uncommitted changes detected. Please commit before release.' && exit 1) && standard-version && git push --follow-tags && npm publish --access public",
    "storybook": "storybook dev -p 6006"
  }
}
